{
    "name": "larium/odm",
    "description": "Object document mapper",
    "keywords": ["odm"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Andreas Kollaros",
            "email": "andreas@larium.net"
        }
    ],
    "require": {
        "php": ">=7.4",
        "doctrine/collections": "^1.6",
        "doctrine/annotations": "^1.7"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.4",
        "microsoft/azure-storage-table": "~1.1.3",
        "google/cloud-firestore": "^1.7"
    },
    "suggest": {
        "google/cloud-firestore": "",
        "microsoft/azure-storage-table": ""
    },
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Larium\\ODM\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Larium\\ODM\\": "tests/"
        }
    }
}
